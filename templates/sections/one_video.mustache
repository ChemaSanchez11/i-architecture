<section data-section="{{id}}" class="section video-left-text-right">
    <div class="container-fluid px-0">
        <div class="w-100" style="{{s_css}}">
            {{#items}}
                {{#type}}
                    {{#is_video}}
                        <video src="{{#print_img}}{{media_url}}{{/print_img}}" style="{{css}}" class="img-fluid d-block" autoplay muted loop playsinline></video>
                    {{/is_video}}
                {{/type}}
            {{/items}}
        </div>
    </div>
</section>